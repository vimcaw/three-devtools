{
  "name": "embedding",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "src"
  ],
  "main": "dist/index.js",
  "module": "src/main.tsx",
  "types": "./dist/packages/embedding/src/main.d.ts",
  "exports": {
    "types": "./src/main.tsx",
    "import": "./dist/index.js"
  },
  "scripts": {
    "dev": "vite",
    "build:type": "dts-bundle-generator -o dist/main.d.ts src/main.tsx --external-imports three --external-types --no-check",
    "build:post": "tsx scripts/postBuild.ts",
    "build": "vite build && pnpm build:type && pnpm build:post",
    "lint": "tsc",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm"
  },
  "publishConfig": {
    "directory": "./dist",
    "registry": "https://registry.npmjs.org/",
    "tag": "alpha",
    "access": "public"
  },
  "dependencies": {
    "core": "workspace:*",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "shared": "workspace:*",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.15.13",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "dts-bundle-generator": "^8.0.1",
    "sort-package-json": "^2.4.1",
    "tsx": "^3.12.6",
    "type-fest": "^3.8.0",
    "vite": "^4.2.0"
  }
}
